library(ggplot2)
mpg<-as.data.frame(ggplot2::mpg)
library(dplyr)
mpg<-as.data.frame(ggplot2::mpg)
mpg_tdata <- mpg %>% select(class,cty) %>% filter(class %in% c("compact","suv"))
head(mpg_tdata)
table(mpg_tdata)
table(mpg_tdata$class)
t.test(data=mpg_tdata,cty~class,var.equal = T)
mpg_tdata2 <- mpg %>% select(fl,cty) %>% filter(fl %in% c("r","p"))
head(mpg_tdata2)
table(mpg_tdata2)
table(mpg_tdata2$fl)
t.test(data=mpg_tdata2,cty~fl,car.equal = T)
t.test(data=mpg_tdata2,cty~fl,var.equal = T)
